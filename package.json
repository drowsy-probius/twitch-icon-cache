{
  "name": "twitch-icon-cache",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/k123s456h/twitch-icon-cache.git",
  "author": "k123s456h <k123s456h@gmail.com>",
  "license": "MIT",
  "scripts": {
    "install": "./scripts/install-dependencies.sh",
    "dev": "concurrently --kill-others-on-fail \"yarn dev:back\" \"yarn dev:front\"",
    "start": "concurrently --kill-others-on-fail \"yarn start:back\" \"yarn start:front\"",
    "build": "concurrently --kill-others-on-fail \"yarn build:back\" \"yarn build:front\"",
    "dev:back": "cd backend && yarn dev",
    "dev:front": "cd frontend && yarn dev",
    "build:back": "cd backend && yarn build",
    "build:front": "cd frontend && yarn build",
    "start:back": "cd backend && yarn start",
    "start:front": "cd frontend && yarn start"
  },
  "devDependencies": {
    "concurrently": "^7.4.0"
  }
}
